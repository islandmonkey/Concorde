  <!--=== SPEED DISPLAY ===-->
  <!-- IH -->
  <animation>
    <type>material</type>
    <object-name>IH</object-name>
    <condition>
      <and>
        <equals>
          <property>controls/autoflight/speed-display</property>
          <value>IH</value>
        </equals>
      </and>
    </condition>
    <emission>
      <red>0.5</red>
      <green>0.1</green>
      <blue>0.0</blue>
    </emission>
  </animation>
