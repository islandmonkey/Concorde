    </axis>
  </animation>
  <!-- Speed Select -->
  <animation>
    <type>rotate</type>
    <object-name>SpeedSelect</object-name>
    <property>controls/autoflight/speed-select</property>
    <factor>1</factor>
    <center>
      <x-m>0.145</x-m>
      <y-m>-0.126</y-m>
      <z-m>-0.025</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>pick</type>
    <object-name>SpeedSelect</object-name>
    <action>
      <name>Speed 1 Up</name>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>controls/autoflight/speed-select</property>
        <step>1</step>
        <min>120</min>
        <max>400</max>
        <wrap>false</wrap>
      </binding>
    </action>
    <action>
      <name>Speed 1 Down</name>
      <button>1</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>controls/autoflight/speed-select</property>
        <step>-1</step>
        <min>120</min>
        <max>400</max>
        <wrap>false</wrap>
      </binding>
    </action>
    <action>
      <name>Speed 10 Up</name>
      <button>3</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>controls/autoflight/speed-select</property>
        <step>10</step>
        <min>120</min>
        <max>400</max>
        <wrap>false</wrap>
      </binding>
    </action>
    <action>
      <name>Speed 10 Down</name>
      <button>4</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>controls/autoflight/speed-select</property>
        <step>-10</step>
        <min>120</min>
        <max>400</max>
        <wrap>false</wrap>
      </binding>
    </action>
  </animation>
