<?xml version="1.0"?>
<PropertyList>
  <path>../Models/takeoffmonitor.ac</path>
  <animation>
    <type>material</type>
    <object-name>Button</object-name>
    <condition>
         <property>/instrumentation/takeoff-monitor/armed</property>
    </condition>
    <emission>
      <red>0.8</red>
      <green>0.8</green>
      <blue>0.8</blue>
    </emission>
  </animation>
    <animation>
    <type>pick</type>
    <object-name>Button</object-name>
    <action>
      <name>Toggle</name>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>/instrumentation/takeoff-monitor/armed</property>
      </binding>
          <binding>
        <command>property-toggle</command>
        <property>/instrumentation/generic/click</property>
      </binding>
      </action>
    </animation>
      <!--Default Lighting-->
  <animation>
    <type>material</type>
    <object-name>Frame</object-name>
    <emission>
      <red>0.01</red>
      <green>0.005</green>
      <blue>0.005</blue>
      <factor-prop>controls/lighting/panel-norm</factor-prop>
    </emission>
  </animation>
