<?xml version="1.0"?>
<PropertyList>
  <path>../Models/cabin-alt.ac</path>
  <params>
    <altitude>instrumentation/cabin-altitude[0]/indicated-altitude-ft</altitude>
  </params>
  <animation>
    <type>rotate</type>
    <object-name>Needle</object-name>
    <property>/instrumentation/cabin-altitude[0]/indicated-altitude-ft</property>
    <interpolation>
      <entry>
        <ind>-5000</ind>
        <dep>-30</dep>
      </entry>
      <entry>
        <ind>20000</ind>
        <dep>120</dep>
      </entry>
      <entry>
        <ind>30000</ind>
        <dep>165</dep>
      </entry>
      <entry>
        <ind>40000</ind>
        <dep>195</dep>
      </entry>
      <entry>
        <ind>50000</ind>
        <dep>210</dep>
      </entry>
    </interpolation>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <!--Default Lighting-->
  <animation>
    <type>material</type>
    <object-name>Guage</object-name>
    <emission>
      <red>0.2</red>
      <green>0.1</green>
      <blue>0.1</blue>
      <factor-prop>controls/lighting/panel-norm</factor-prop>
    </emission>
  </animation>
  <animation>
    <type>material</type>
    <object-name>Frame</object-name>
    <emission>
      <red>0.01</red>
      <green>0.005</green>
      <blue>0.005</blue>
      <factor-prop>controls/lighting/panel-norm</factor-prop>
    </emission>
  </animation>
  <animation>
    <type>material</type>
    <object-name>Needle</object-name>
    <emission>
      <red>0.1</red>
      <green>0.03</green>
      <blue>0.03</blue>
      <factor-prop>controls/lighting/panel-norm</factor-prop>
    </emission>
  </animation>
</PropertyList>
